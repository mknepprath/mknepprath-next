    "date-fns": "^2.24.0",
    "isomorphic-unfetch": "^3.1.0",
    "markov-strings": "^3.0.1",
    "mcproto": "^0.9.0",
    "next": "^11.1.2",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring": "^9.2.4",
    "react-use-gesture": "^9.1.3",
    "swr": "^0.5.6",
